<div id="myCarousel" class="carousel slide " data-ride="carousel">
    <ol class="carousel-indicators">
        <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
        <li data-target="#myCarousel" data-slide-to="1"></li>
        <li data-target="#myCarousel" data-slide-to="2"></li>
    </ol>
    <!-- Wrapper for slides -->
    <div class="carousel-inner carousel-out">
        <div class="item active img-item1">
            <img src="/assets/static/images/Home-Carousel-01.jpg" alt="Los Angeles">
            <img src="/assets/static/images/Home-Carousel-01-sm.jpg" alt="Los Angeles">
        </div>

        <div class="item img-item3">
            <img src="/assets/static/images/Home-Carousel-02.jpg" alt="Chicago">
            <img src="/assets/static/images/Home-Carousel-02-sm.jpg" alt="Chicago">
        </div>

        <div class="item img-item3">
            <img src="/assets/static/images/Home-Carousel-03.jpg" alt="New York">
            <img src="/assets/static/images/Home-Carousel-03-sm.jpg" alt="New York">
        </div>
    </div>
    <!-- Left and right controls -->
    <a class="left carousel-control" href="#myCarousel" role="button" data-slide="prev">
        <span class="icon-prev" aria-hidden="true"></span>
        <span class="sr-only">Previous</span>
    </a>
    <a class="right carousel-control" href="#myCarousel" role="button" data-slide="next">
        <span class="icon-next" aria-hidden="true"></span>
        <span class="sr-only">Next</span>
    </a>
</div>